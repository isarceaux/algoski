 <!-- Aside Start-->
<aside class="left-panel">

    <!-- brand -->
    <div class="logo">
        
        <%= link_to pages_home_path, class:"logo-expanded" do %>
            <!-- <i class="ion-social-buffer"></i> -->
            <%= image_tag('logo-white.png', size:'30')%>
            <span class="nav-label nav-title">Algoski</span>
        <% end %>
    </div>
    <!-- / brand -->

    <!-- Navbar Start -->
    <nav class="navigation">
        <ul class="list-unstyled">

            <% if current_user.account != nil && current_user.subscriptions.count > 0 %>

                <!-- Analysis - Map - Table -->
                <li class=<%= cp(dashboards_analysis_path) %> >
                <%= link_to dashboards_analysis_path do %>
                    <i class="ion-stats-bars"></i> 
                    <span class="nav-label">Evolution des prix</span>
                <% end %>
                </li>

                <li class=<%= cp(dashboards_map_path) %>>
                <%= link_to dashboards_map_path do %>
                    <i class="ion-location"></i> 
                    <span class="nav-label">Carte des concurrents</span>
                <% end %>
                </li>

                <li class=<%= cp(dashboards_data_table_path) %>>
                <%= link_to dashboards_data_table_path do %>
                    <i class="ion-grid"></i> 
                    <span class="nav-label">Tableau des concurrents</span>
                <% end %>
                </li>
                <!-- end of analysis - map - table -->

            <% end %>

            <!-- Mon compte -->
            <li class=<%= cp(dashboards_profile_path) %>>
            <%= link_to dashboards_profile_path do %>
                <i class="ion-person"></i> 
                <span class="nav-label">Mon compte</span>
            <% end %>
            </li>
            <!-- end of mon compte -->

        </ul>
    </nav>
        
</aside>
<!-- Aside Ends