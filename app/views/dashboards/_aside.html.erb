<!-- Aside Start-->
<aside class="left-panel">

    <!-- brand -->
    <div class="logo">
        
        <%= link_to pages_home_path, class:"logo-expanded" do %>
            <!-- <i class="ion-social-buffer"></i> -->
            <%= image_tag('logo-white.png', size:'30')%>
            <span class="nav-label nav-title">Algoski</span>
        <% end %>
    </div>
    <!-- / brand -->

    <!-- Navbar Start -->
    <nav class="navigation">
        <ul class="list-unstyled">
            <li class="active">
            <%= link_to dashboards_analysis_path do %>
                <i class="ion-stats-bars"></i> 
                <span class="nav-label">Analyses</span>
            <% end %>
            </li>

            <li>
            <%= link_to '#' do %>
                <i class="ion-location"></i> 
                <span class="nav-label">Carte</span>
            <% end %>
            </li>

            <li>
            <%= link_to '#' do %>
                <i class="ion-grid"></i> 
                <span class="nav-label">Tableau</span>
            <% end %>
            </li>

            <% if user_signed_in? %>
                <li>
                <%= link_to edit_user_registration_path do %>
                    <i class="ion-person"></i> 
                    <span class="nav-label">Mon compte</span>
                <% end %>
                </li>
            <% else %>
                <li>
                    <%= link_to new_user_session_path do %>
                    <i class="ion-person"></i> 
                    <span class="nav-label">Se connecter</span>
                    <% end %>
                </li>
            <% end %>
        </ul>
    </nav>
        
</aside>
<!-- Aside Ends-->